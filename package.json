{
  "name": "myreactapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --port 2145 --mode development",
    "pro": "vite --port 3145 --mode production",
    "build": "npm run build:dev",
    "build:dev": "rimraf dist-env-development && tsc && vite build --mode development",
    "build:pro": "rimraf dist-env-production && tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.8.0",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "sass": "^1.64.2",
    "styled-components": "^6.0.5",
    "stylus": "^0.59.0"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.3",
    "axios": "^1.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "mobx": "^6.10.0",
    "mobx-react": "^9.0.0",
    "rimraf": "^5.0.1",
    "terser": "^5.19.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
